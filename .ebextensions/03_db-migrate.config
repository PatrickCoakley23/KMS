container_commands:
  01_migrate:
    command: "django-admin.py migrate"
    leader_only: true
  02_createsuperuser:
    command: "python manage.py createsu"
    leader_only: true
  03_collect_static:
    command: "django-admin.py collectstatic --noinput" 
    leader_only: true


option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: KMS.settings